<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="mass_update_warranty_view_form" model="ir.ui.view">
        <field name="name">mass.update.warranty.view.form</field>
        <field name="model">mass.update.warranty</field>
        <field name="arch" type="xml">
            <form string="Mass Update Warranty Form">
                <sheet>
                    <group>
                        <field name="date_from"/>
                        <field name="date_to"/>
                    </group>
                    <footer>
                        <button name="mass_update_warranty" string="Update" type="object"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

    <record id="res_partner_act_window_mass_update" model="ir.actions.act_window">
        <field name="name">Mass Update Warranty</field>
        <field name="res_model">mass.update.warranty</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="binding_model_id" ref="product.model_product_template"/>
        <field name="binding_view_types">list</field>
    </record>
</odoo>